# Database Configuration
DATABASE_URL=postgresql://user:password@host:port/database
CORS_ORIGINS=*
SQL_ECHO=false

# FRED API (Federal Reserve Economic Data) - For macro economic indicators
# Get a free API key from: https://fredaccount.stlouisfed.org
# Rate limit: 2,000 requests/day (more than enough)
FRED_API_KEY=your_32_character_fred_api_key_here

# Email Notification Settings (Gmail)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-gmail-app-password

# Twilio Settings (for SMS and WhatsApp)
TWILIO_ACCOUNT_SID=your-account-sid
TWILIO_AUTH_TOKEN=your-auth-token
TWILIO_PHONE_NUMBER=+1234567890
